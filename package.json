{
  "name": "@blazesrus/vike-svelte",
  "workspaces": [
    "packages/@blazesrus/vike-svelte"
  ],
  "scripts": {
    "dev": "cd packages/@blazesrus/vike-svelte/ && pnpm run dev",
    "build": "cd packages/@blazesrus/vike-svelte/ && pnpm run build",
    "========= Clean": "",
    "clean": "git clean -Xdf",
    "reset": "pnpm run clean && pnpm install && pnpm run build",
    "========= Release": "",
    "release": "cd packages/@blazesrus/vike-svelte/ && pnpm run release",
    "release:commit": "cd packages/@blazesrus/vike-svelte/ && pnpm run release:commit",
    "========= Only allow pnpm; forbid yarn & npm": "",
    "preinstall": "npx only-allow pnpm"
  },
  "packageManager": "pnpm@10.13.1"
}